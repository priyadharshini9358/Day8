public class method {
	method()
	{
		System.out.println("hi");
	}
	int age;
	String name;
	method(int a ,String str)
	{
		age=a;
		name=str;
	}
	void display()
	{
		System.out.println(age +"  "+ name);
	}
	public static void main(String[] args) {
		method obj=new method(5,"riya");
		obj.display();
	}
}
